  },
  "homepage": "https://github.com/AJChapp/Web-Scraper-Now-Featuring-Style#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "cheerio": "^1.0.0-rc.2",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "mongoose": "^5.3.5"
  }
}
